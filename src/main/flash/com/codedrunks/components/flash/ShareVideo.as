package com.codedrunks.components.flash{	import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	public class ShareVideo extends MovieClip	{		private var view:MissionArmy;		private var sharedVideoMc:SharedVideoMc;				public function ShareVideo(view:MissionArmy)		{			this.view = view;		}				public function initialize():void		{			sharedVideoMc = new SharedVideoMc();			view.stage.addChild(sharedVideoMc);			sharedVideoMc.closeShareWindowBtn.addEventListener(MouseEvent.CLICK,handleCloseSharedWindowBtn);		}				private function handleCloseSharedWindowBtn(event:Event):void		{			view.stage.removeChild(sharedVideoMc);			sharedVideoMc.sharedVideoSlideshow.muteMedia();			view.jumpToMakeYourOwnVideoTab();			//view.initApp();		}	}}